{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://lichen.system/schemas/rag_build/EmbeddingJob.schema.json",
  "title": "EmbeddingJob",
  "type": "object",
  "required": ["job_id", "embed_model", "embedding_dim", "normalize", "batch_size", "chunkset_uri", "output_uri"],
  "properties": {
    "job_id": { "type": "string" },
    "embed_model": { "type": "string" },
    "embedding_dim": { "type": "integer", "minimum": 1 },
    "normalize": { "type": "boolean" },
    "batch_size": { "type": "integer", "minimum": 1 },
    "precision": { "enum": ["fp32", "fp16", "bf16"] },
    "seed": { "type": "integer" },
    "chunkset_uri": { "type": "string" },
    "output_uri": { "type": "string" },
    "created_at": { "type": "string", "format": "date-time" }
  },
  "additionalProperties": false
}
